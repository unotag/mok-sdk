{
  "compilerOptions": {
    "target": "ES2019",
		"lib": ["ES2019", "DOM"],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "jsx": "react",
    "skipLibCheck": true
  },
}