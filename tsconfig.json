{
  "compilerOptions": {
    "target": "ES2019",
		"lib": ["ES2019"],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noImplicitAny": true
  },
  "include": [
    "src/**/*"
  ]
}